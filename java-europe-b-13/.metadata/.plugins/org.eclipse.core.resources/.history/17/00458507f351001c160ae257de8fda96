package day37_inheritance;

public class Company {

	public static void main(String[] args) {
	
		
		Empleyee emp1 = new Empleyee();
		
		emp1.setAge(10);
		emp1.setName("Mike");
		System.out.println(emp1.getAge());
	  	emp1.jobTitle = "SDET";
		
		emp1.gender='M';
		
		emp1.work();
		emp1.sleep(10);
		emp1.eat("Pizza");
		
		
		

	}

}
